# Chrome Web Store
CHROME_WEBSTORE_CLIENT_ID=your_client_id
CHROME_WEBSTORE_CLIENT_SECRET=your_secret
CHROME_WEBSTORE_REFRESH_TOKEN=your_refresh_token

# Stripe
STRIPE_API_KEY=your_stripe_key
